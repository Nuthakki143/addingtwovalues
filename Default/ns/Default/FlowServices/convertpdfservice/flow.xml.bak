            font-family:verdana;

            font-style:italic;

        }

 

 

        /* Styling for the right corner "AVIS | iLEASE" */

        .right-corner {

            font-size: 25px;

            background-color: rgb(190,0,0);

            color: white;

            font-weight: bold;

            font-family:verdana;

            padding: 5px;

        }

 

 

        /* Styling for the line under "QUOTATION REQUEST FORM - iLease" */

        .iLease{

        	font-family:verdana;

            font-style:italic;

            }


        .underline {

            text-align: left;

            margin-top: 10px;

            margin-bottom: 25px;

        }


 

        /* Styling for the form labels */

        .underlined {

            margin-right: 10px;

            font-weight: normal;


        }

 

		 /* Styling for the big, dark, bold line */

        .big-line {

            border-bottom: 5px solid black;

        }


  		 /* Styling for "Compulsory Value-Added Products" */

        .compulsory-products {

            font-weight: bold;

            margin-top:15px;

            margin-bottom:10px;

            font-size:14px;

            font-family:verdana;

            font-style:italic;

        }

 

 

        /* Styling for "Vehicle Details (2 selections per quote request)" */

        .vehicle-details {

            text-decoration: underline;

            font-weight: bold;

            margin-top: 10px;

            margin-bottom: 10px;

        }

 

 

        /* Styling for the quote details */

        .quote-details {

            font-size: 15px;

            color: rgb(180,180,180);

            margin-bottom: 10px;

        }


 

        /* Styling for the M &amp; M Code section */

        .mm-code-label {

            margin-bottom: 10px;

            font-weight: normal;

        }



        .vehicleUnderline {

        	margin-left: 30px;


        }


&lt;/style&gt;

